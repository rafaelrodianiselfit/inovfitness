SELECT 
  NULL AS ID_CLIENT,
  EF.ID_FILIAL AS ID_CLUB,
  'SELFIT ACADEMIAS' AS BRAND,
  NULL AS BRAND_SHORT,
  EF.NOME AS NAME,
  NULL AS NAME_SHORT,
  NULL AS POSTAL_CODE,
  EF.CIDADE AS CITY,
  UF.NOME AS REGION,
  P.NOME AS COUNTRY,
  EF.LAT AS LATITUDE,
  EF.LON AS LONGITUDE,
  NULL AS PHONE_NUMBER,
  NULL AS EMAIL,
  EF.DT_INAUGURACAO AS OPENING_DATE,
  NULL AS CLUB_TYPE,
  NULL AS CLUB_TYPE_DESCRIPTION,
  NULL AS CLUB_TYPE_LEVEL,
  NULL AS MAX_CAPACITY,
  NULL AS FLG_ACTIVE,
  NULL AS FLG_DELETED,
  EF.DT_INAUGURACAO AS CREATED_AT,
  NULL AS LAST_MODIFIED_AT
 FROM EMPRESAS_FILIAIS EF
 LEFT JOIN ESTADOS UF ON EF.ID_ESTADO = UF.ID_ESTADO 
 LEFT JOIN PAIS P ON EF.ID_PAIS = P.ID_PAIS 
 WHERE EF.ID_FILIAL <> 1001
